# Telegram bot token issued by BotFather
TELEGRAM_BOT_TOKEN=replace-with-telegram-token

# OpenRouter API key for LLM access
OPENROUTER_API_KEY=replace-with-openrouter-key

# Optional: override default model (defaults to openai/gpt-oss-20b:free)
OPENROUTER_MODEL=openai/gpt-oss-20b:free

# Optional: adjust logging verbosity (INFO, DEBUG, WARNING, ERROR)
LOG_LEVEL=INFO
